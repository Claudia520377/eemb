          <navigator url="/pages/footprint/footprint" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
              <view class="weui-cell__hd">
                  <image src="http://eemb.cn/wx/assets/profile5.png" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
              </view>
              <view class="weui-cell__bd">我的足迹</view>
              <view class="weui-cell__ft weui-cell__ft_in-access"></view>
          </navigator>
        </view>
        <view class='line-width30'></view>
        <view class="weui-cells weui-cells_after-title">
          <navigator url="/pages/recharge/recharge" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
            <view class="weui-cell__hd">
                <image src="http://eemb.cn/wx/assets/profile6.png" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
            </view>
            <view class="weui-cell__bd">充值记录</view>
            <view class="weui-cell__ft weui-cell__ft_in-access"></view>
          </navigator>
        </view>
        <view class='line-width30'></view>
        <view class="weui-cells weui-cells_after-title">
          <navigator url="/pages/address/address" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
              <view class="weui-cell__hd">
                  <image src="http://eemb.cn/wx/assets/profile7.png" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
              </view>
              <view class="weui-cell__bd">收货地址管理</view>
              <view class="weui-cell__ft weui-cell__ft_in-access"></view>
          </navigator>
          <navigator url="/pages/InformationSettings/InformationSettings" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
              <view class="weui-cell__hd">
                  <image src="http://eemb.cn/wx/assets/profile8.png" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
              </view>
              <view class="weui-cell__bd">会员资料设置</view>
              <view class="weui-cell__ft weui-cell__ft_in-access"></view>
          </navigator>
        </view>
        <view class='line-width30'></view>
